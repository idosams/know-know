{
  "name": "@knowgraph/cli",
  "version": "0.2.0",
  "description": "CLI tool for KnowGraph",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idosams/know-know.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/idosams/know-know#readme",
  "bugs": {
    "url": "https://github.com/idosams/know-know/issues"
  },
  "keywords": [
    "knowgraph",
    "cli",
    "code-knowledge",
    "metadata",
    "ai",
    "mcp"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "knowgraph": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@knowgraph/core": "workspace:*",
    "@knowgraph/mcp-server": "workspace:*",
    "chalk": "^5.4.0",
    "commander": "^13.0.0",
    "inquirer": "^12.0.0",
    "ora": "^8.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0"
  }
}
